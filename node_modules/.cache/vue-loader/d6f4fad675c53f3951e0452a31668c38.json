C,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACnB","file":"/home/abdul-muqeet/Vue Apps/multiple-image-picker/src/views/Home.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"home\">\n    <form @submit.prevent=\"uploadForm\">\n      <div class=\"base-input\">\n            <BaseInput \n                label=\"Name\"\n                v-model=\"form_data.name\"\n                type=\"text\"\n            />\n      </div>\n      <div class=\"base-input\">\n            <BaseInput \n                label=\"Description\"\n                v-model=\"form_data.description\"\n                type=\"text\"\n            />\n      </div>\n      <div class=\"base-input\">\n            <BaseInput \n                label=\"type\"\n                v-model=\"form_data.type\"\n                type=\"text\"\n            />\n      </div>\n      <div class=\"base-input\">\n            <BaseInput \n                label=\"stock\"\n                v-model=\"form_data.stock\"\n                type=\"text\"\n            />\n      </div>\n      <div class=\"base-input\">\n            <BaseInput \n                label=\"status\"\n                v-model=\"form_data.status\"\n                type=\"text\"\n            />\n        </div>\n      <MultiImagePicker @delete_image=\"deleteImage\" liveImageURL=\"http://192.168.100.212:3000/uploads/\" deleteURL=\"http://192.168.100.212:3000/delete-mage\" dataURL=\"http://localhost:3000/data/\" data_id=\"1\" v-model=\"form_data.image\" />\n      <loader v-if=\"loader\" />\n      <button type=\"submit\">Upload Data</button>\n    </form>\n    \n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport MultiImagePicker from '@/components/MultiImagePicker'\nimport EventService from '@/services/EventService'\nimport Loader from '../components/loader.vue'\nimport BaseInput from '@/components/BaseInput'\n\nexport default {\n  name: 'Home',\n  components: {\n    MultiImagePicker,\n    Loader,\n    BaseInput,\n  },\n  data(){\n    return {\n      form_data: {\n        name:'',\n        description:'',\n        type:'',\n        stock:'',\n        status:'',\n        image: null,\n      },\n      loader: false,\n    }\n  },\n  beforeCreate(){\n    EventService.getData(1)\n        .then((result) => {\n              this.form_data=result.data\n        }).catch((err) => {\n            console.log(err)\n        });\n  },\n  \n  methods: {\n    uploadForm(){\n      // this.loader=true\n      EventService.uploadData(this.form_data,\"1\")\n        .then((result) => {\n          console.log(result)\n          this.loader=false\n        }).catch((err) => {\n          console.log(err)\n          this.loader=false\n        });\n      // console.log(this.image)\n    },\n    deleteImage(value){\n      console.log(value)\n    }\n  }\n}\n</script>\n<style>\n.base-input{\n    display: flex;\n    text-align: start;\n    width: 50%;\n    margin: 30px auto;\n    flex-direction: column;\n}\nbutton{\n    background: #222;\n    color: #fff;\n    padding: 10px 25px;\n    border-radius: 10px;\n    cursor: pointer;\n}\n</style>"]}]}