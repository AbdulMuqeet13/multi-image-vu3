s.data_id = this.$route.params.id\n    console.log(this.data_id)\n    EventService.getData(this.$route.params.id)\n        .then((result) => {\n              this.form_data=result.data\n        }).catch((err) => {\n            console.log(err)\n        });\n  },\n  \n  methods: {\n    async uploadForm(){\n      this.loader=true\n      await EventService.uploadData(this.form_data,this.$route.params.id)\n      console.log('awaitng......')\n      // await axios.get('http://192.168.100.212:3000/muqeet/'+this.$route.params.id)\n      //     .then((result)=>{\n      //       console.log(result)\n      //       result.data.image.forEach(elem=>{\n      //         this.form_data.image.push(elem)\n      //         })\n      //     })\n      this.loader=false\n      // console.log(this.image)\n    }\n  }\n}\n</script>\n<style>\n.base-input{\n    display: flex;\n    text-align: start;\n    width: 50%;\n    margin: 30px auto;\n    flex-direction: column;\n}\nbutton{\n    background: #222;\n    color: #fff;\n    padding: 10px 25px;\n    border-radius: 10px;\n    cursor: pointer;\n    margin: 10px;\n}\n.loader-div{\n  position: fixed;\n    left: 0%;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    background: rgba(0, 0, 0, 0.5);\n}\n</style>"],"sourceRoot":""}]}